<p id="notice"><%= notice %></p>

<%= link_to 'New Artwork', new_artwork_path %>

<h1>Listing Artworks</h1>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Title</th>
      <th>Price</th>
      <th>Date</th>
      <th>Description</th>
      <th>Period</th>
      <th>Edition</th>
      <th>Medium</th>
      <th>Size</th>
      <th>Authentic</th>
      <th>Stock</th>
      <th>Subject</th>
      <th>Artwork artist map</th>
      <th>Artwork seller map</th>
      <th>Artwork image map</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @artworks.each do |artwork| %>
      <tr>
        <td><%= artwork.id %></td>
        <td><%= artwork.title %></td>
        <td><%= artwork.price %></td>
        <td><%= artwork.date %></td>
        <td><%= artwork.description %></td>
        <td><%= artwork.period %></td>
        <td><%= artwork.edition %></td>
        <td><%= artwork.medium %></td>
        <td><%= artwork.size %></td>
        <td><%= artwork.authentic %></td>
        <td><%= artwork.stock %></td>
        <td><%= artwork.subject %></td>
        <td><%= artwork.artwork_artist_map_id %></td>
        <td><%= artwork.artwork_seller_map_id %></td>
        <td><%= artwork.artwork_image_map_id %></td>
        <td><%= link_to 'Show', artwork %></td>
        <td><%= link_to 'Edit', edit_artwork_path(artwork) %></td>
        <td><%= link_to 'Destroy', artwork, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


