<p id="notice"><%= notice %></p>

<h1>Artwork Seller Maps Table</h1>

<div class="container">
  <div class="row">
    <div class="col-md-8 col-sm-8">
      <%= will_paginate @artwork_seller_maps %>
    </div>
    <div class="col-md-4 col-sm-4 newBtn">
      <%= link_to 'New Artwork seller map', new_artwork_seller_path, :class=>"btn btn-primary"%>
    </div>
  </div>
</div>

<div class="table-responsive">
  <table class="table table-bordered fixed">
    <thead>
      <tr>
        <th>ID</th>
        <th>Artwork ID</th>
        <th>Seller ID</th>
        <th colspan="3">Operations</th>
      </tr>
    </thead>

    <tbody>
      <% @artwork_seller_maps.each do |artwork_seller_map| %>
        <tr>
          <td><%= artwork_seller_map.id %></td>
          <td><%= artwork_seller_map.artwork_id %></td>
          <td><%= artwork_seller_map.seller_id %></td>
          <td><%= link_to 'Show', artwork_seller_map %></td>
          <td><%= link_to 'Edit', edit_artwork_seller_map_path(artwork_seller_map) %></td>
          <td><%= link_to 'Destroy', artwork_seller_map, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>

