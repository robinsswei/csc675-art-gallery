<p id="notice"><%= notice %></p>

<%= link_to 'New Seller', new_seller_path %>

<h1>Listing Sellers</h1>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Type</th>
      <th>Organization</th>
      <th>First name</th>
      <th>Last name</th>
      <th>Phone number</th>
      <th>Address</th>
      <th>Email</th>
      <th>Country</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sellers.each do |seller| %>
      <tr>
        <td><%= seller.id %></td>
        <td><%= seller.seller_type %></td>
        <td><%= seller.organization_name %></td>
        <td><%= seller.first_name %></td>
        <td><%= seller.last_name %></td>
        <td><%= seller.phone_number %></td>
        <td><%= seller.address %></td>
        <td><%= seller.email %></td>
        <td><%= seller.country_id %></td>
        <td><%= link_to 'Show', seller %></td>
        <td><%= link_to 'Edit', edit_seller_path(seller) %></td>
        <td><%= link_to 'Destroy', seller, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

